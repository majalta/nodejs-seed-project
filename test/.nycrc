{
    "check-coverage": true,
    "per-file": false,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js"
    ],
    "ignore-class-method": "",
    "reporter": [
      "html",
      "text"
    ],
    "require": [
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./test/coverage"
  }
